#include<stdio.h>
struct employee{
    char name [25];
    int id;
    char department[20];
    float salary;
    char email[50];
};
int main() {
    struct employee emp = {
        "John Doe",
        12345,
        "human resources",
        55000.50,
        "jonh.doe@company.com"
    };
    printf("Name: %s\n",emp.name);
    printf("id: %d\n",emp.id);
    printf("department: %s\n",emp.department);
    printf("salary: %f\n",emp.salary);
    printf("email: %s\n",emp.email);
    return 0;
}
